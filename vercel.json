{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    },
    {
      "src": "Frontend/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/admin.html",
      "dest": "Frontend/admin.html"
    },
    {
      "src": "/script.js",
      "dest": "Frontend/script.js"
    },
    {
      "src": "/(.*\\.)(js|css|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot)$",
      "dest": "Frontend/$1"
    },
    {
      "src": "/",
      "dest": "Frontend/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "Frontend/index.html"
    }
  ]
}
