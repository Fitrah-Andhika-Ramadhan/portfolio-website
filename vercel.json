{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "Frontend/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "Frontend/admin.html",
      "use": "@vercel/static"
    },
    {
      "src": "Frontend/script.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/admin.html",
      "dest": "Frontend/admin.html"
    },
    {
      "src": "/script.js",
      "dest": "Frontend/script.js"
    },
    {
      "src": "/",
      "dest": "Frontend/index.html"
    }
  ]
}
